		"jest": "^25.1.0",
		"strip-ansi": "^6.0.0",
		"ts-jest": "^25.2.1"
	},
	"dependencies": {
		"@as-pect/assembly": "^3.0.0-alpha.3",
		"chalk": "^3.0.0",
		"long": "^4.0.0"
	},
	"files": [
		"init/",
		"lib/"
	],
	"peerDependencies": {
		"assemblyscript": "0.9.2"
