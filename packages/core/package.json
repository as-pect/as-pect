{
	"name": "@as-pect/core",
	"version": "3.2.7",
	"description": "Write AssemblyScript tests at blazing fast speeds",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/jtenner/as-pect/tree/master/packages/core"
	},
	"author": "Joshua Tenner <tenner.joshua@gmail.com>",
	"license": "MIT",
	"keywords": [
		"wasm",
		"testing",
		"typescript",
		"assemblyscript"
	],
	"scripts": {
		"pretest:all": "run-p pretest:group-filter",
		"pretest:group-filter": "asc --debug --binaryFile assembly/jest-filter.wasm --textFile assembly/jest-filter.wat --explicitStart ./assembly/jest-filter.ts ../assembly/assembly/index.ts --transform ./lib/transform/index.js",
		"rimraf": "rimraf lib/*",
		"test:ci": "node --experimental-wasi-unstable-preview1 --experimental-wasm-bigint ./node_modules/jest/bin/jest.js",
		"test": "node --experimental-wasi-unstable-preview1 --experimental-wasm-bigint ./node_modules/jest/bin/jest.js",
		"tsc:all": "run-p tsc:default tsc:amd",
		"tsc:amd": "tsc --outfile lib/as-pect.core.amd.js --module amd --declaration --declarationMap",
		"tsc:default": "tsc",
		"test:update-snapshots": "node --experimental-wasi-unstable-preview1 --experimental-wasm-bigint ./node_modules/jest/bin/jest.js -u",
		"tsc": "run-s rimraf tsc:all"
	},
	"bugs": {
		"url": "https://github.com/jtenner/aspect/issues"
	},
	"homepage": "https://github.com/jtenner/as-pect#readme",
	"dependencies": {
		"@as-pect/assembly": "^3.2.7",
		"@as-pect/snapshots": "^3.2.7",
		"chalk": "^4.1.0",
		"long": "^4.0.0"
	},
	"files": [
		"init/",
		"lib/"
	],
	"peerDependencies": {
		"assemblyscript": "^0.10.1"
	},
	"devDependencies": {
		"jest": "^26.0.1",
		"ts-jest": "^26.1.0"
	}
}
