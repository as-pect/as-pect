{
	"name": "@roaminroe/as-pect-core",
	"version": "6.3.3",
	"description": "Write AssemblyScript tests at blazing fast speeds",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/jtenner/as-pect/tree/master/packages/core"
	},
	"author": "Joshua Tenner <tenner.joshua@gmail.com>",
	"license": "MIT",
	"keywords": [
		"wasm",
		"testing",
		"typescript",
		"assemblyscript"
	],
	"scripts": {
		"pretest:reporter-top-level-expect": "asc --debug --textFile assembly/jest-reporter-top-level-expect.wat --exportStart _start ./assembly/jest-reporter-top-level-expect.ts ../assembly/assembly/index.ts --transform ./lib/transform/index.js --use ASC_RTRACE=1",
		"pretest:reporter-snapshot": "asc --debug --textFile assembly/jest-reporter-snapshot.wat --exportStart _start ./assembly/jest-reporter-snapshot.ts ../assembly/assembly/index.ts --transform ./lib/transform/index.js --use ASC_RTRACE=1",
		"pretest:reporter-flow-error": "asc --debug --textFile assembly/jest-reporter-flow-error.wat --exportStart _start ./assembly/jest-reporter-flow-error.ts ../assembly/assembly/index.ts --transform ./lib/transform/index.js --use ASC_RTRACE=1",
		"pretest:reporter-fail": "asc --debug --textFile assembly/jest-reporter-fail.wat --exportStart _start ./assembly/jest-reporter-fail.ts ../assembly/assembly/index.ts --transform ./lib/transform/index.js --use ASC_RTRACE=1",
		"pretest:reporter-pass": "asc --debug --textFile assembly/jest-reporter-pass.wat --exportStart _start ./assembly/jest-reporter-pass.ts ../assembly/assembly/index.ts --transform ./lib/transform/index.js --use ASC_RTRACE=1",
		"pretest:wasi": "asc --debug --textFile assembly/jest-wasi.wat --exportStart _start ./assembly/jest-wasi.ts ../assembly/assembly/index.ts --transform ./lib/transform/index.js --use ASC_RTRACE=1",
		"pretest:pass-fail": "asc --debug --textFile assembly/jest-pass-fail.wat --exportStart _start ./assembly/jest-pass-fail.ts ../assembly/assembly/index.ts --transform ./lib/transform/index.js --use ASC_RTRACE=1",
		"pretest:group-filter": "asc --debug --textFile assembly/jest-filter.wat --exportStart _start ./assembly/jest-filter.ts ../assembly/assembly/index.ts --transform ./lib/transform/index.js",
		"pretest:log": "asc --debug --textFile assembly/jest-log.wat --exportStart _start ./assembly/jest-log.ts ../assembly/assembly/index.ts --exportTable --use ASC_RTRACE=1 --transform ./lib/transform/index.js",
		"rimraf": "rimraf lib/*",
		"test:ci": "node --experimental-wasi-unstable-preview1 --experimental-vm-modules ./node_modules/jest/bin/jest.js",
		"test": "node --experimental-wasi-unstable-preview1 --experimental-vm-modules ./node_modules/jest/bin/jest.js",
		"test:log": "node --experimental-wasi-unstable-preview1 --experimental-vm-modules ./node_modules/jest/bin/jest.js log",
		"asc:all": "run-p pretest:group-filter pretest:log pretest:pass-fail pretest:wasi pretest:reporter-pass pretest:reporter-fail pretest:reporter-flow-error pretest:reporter-snapshot pretest:reporter-top-level-expect",
		"tsc": "run-p tsc:default tsc:amd",
		"tsc:amd": "tsc --outfile lib/as-pect.core.amd.js --module amd --declaration --declarationMap",
		"tsc:default": "tsc",
		"test:update-snapshots": "node --experimental-wasi-unstable-preview1 --experimental-vm-modules ./node_modules/jest/bin/jest.js -u",
		"tsc:all": "run-s rimraf tsc asc:all"
	},
	"bugs": {
		"url": "https://github.com/jtenner/aspect/issues"
	},
	"homepage": "https://github.com/jtenner/as-pect#readme",
	"dependencies": {
		"@as-pect/assembly": "npm:@roaminroe/as-pect-assembly@^6.3.0",
		"@as-pect/snapshots": "^6.2.1",
		"chalk": "^4.1.1",
		"long": "^4.0.0"
	},
	"files": [
		"init/",
		"lib/"
	],
	"peerDependencies": {
		"assemblyscript": "^0.20.6"
	},
	"devDependencies": {
		"@assemblyscript/loader": "^0.20.6",
		"assemblyscript": "^0.20.6",
		"jest": "^27.0.5",
		"npm-run-all": "^4.1.5",
		"ts-jest": "^27.0.3"
	}
}
