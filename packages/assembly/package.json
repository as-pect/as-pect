{
  "name": "@as-pect/assembly",
  "version": "3.1.2",
  "description": "Write AssemblyScript tests at blazing fast speeds",
  "types": "types/as-pect.d.ts",
  "scripts": {
    "asp:ci": "node ../cli/lib/test --summary",
    "asp:ci-simd": "node ../cli/lib/test --summary --config as-pect.simd.config.js",
    "asp": "node ../cli/lib/test --verbose",
    "asp:simd": "node ../cli/lib/test --verbose --config as-pect.simd.config.js",
    "test:ci": "run-s asp:ci asp:ci-simd",
    "test": "run-s asp asp:simd",
    "tsc": "exit 0",
    "test:update-snapshots": "exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jtenner/as-pect/tree/master/packages/assembly"
  },
  "author": "Joshua Tenner <tenner.joshua@gmail.com>",
  "license": "MIT",
  "keywords": [
    "wasm",
    "assemblyscript",
    "testing"
  ],
  "bugs": {
    "url": "https://github.com/jtenner/aspect/issues"
  },
  "homepage": "https://github.com/jtenner/as-pect#readme",
  "files": [
    "assembly/internal/",
    "assembly/index.ts",
    "types/"
  ],
  "peerDependencies": {
    "assemblyscript": "0.9.4-nightly.20200402"
  }
}
