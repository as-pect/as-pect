{
  "name": "@as-pect/cli",
  "version": "6.3.0",
  "description": "Write AssemblyScript tests at blazing fast speeds",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "scripts": {
    "rimraf": "rimraf lib/*",
    "package:version-update": "ncu -u",
    "test:ci": "jest",
    "test": "jest",
    "test:update-snapshots": "jest -u",
    "tsc:all": "run-p tsc:default",
    "tsc:amd": "tsc --outfile lib/as-pect.cli.amd.js --module amd --declaration --declarationMap",
    "tsc:default": "tsc",
    "tsc": "run-s rimraf tsc:all"
  },
  "bin": {
    "asp": "bin/asp.js",
    "aspect": "bin/asp.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jtenner/as-pect/tree/master/packages/cli"
  },
  "author": "Joshua Tenner <tenner.joshua@gmail.com>",
  "license": "MIT",
  "keywords": [
    "wasm",
    "assemblyscript",
    "testing",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/jtenner/aspect/issues"
  },
  "homepage": "https://github.com/jtenner/as-pect#readme",
  "dependencies": {
    "@as-covers/core": "^0.3.0",
    "@as-pect/assembly": "^6.3.0",
    "@as-pect/core": "^6.3.0",
    "@as-pect/snapshots": "^6.2.1",
    "@assemblyscript/loader": "^0.20.6",
    "chalk": "^4.1.1",
    "glob": "^7.1.7"
  },
  "peerDependencies": {
    "assemblyscript": "^0.20.6"
  },
  "files": [
    "init/",
    "lib/",
    "bin/"
  ],
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
